<Window x:Class="TravianFuryBoarClient.Views.CycleAttack"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravianFuryBoarClient.Views"
        mc:Ignorable="d"
        Title="CycleAttack" Height="450" Width="800">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition></RowDefinition>
			<RowDefinition></RowDefinition>
		</Grid.RowDefinitions>
		<Button Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" Width="300" Height="60" Content="| |" FontSize="30" Command="{ Binding StopCommand}"></Button>
		<Button Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Width="300" Height="60" Content="▶" FontSize="30" Command="{ Binding BeginCommand}"></Button>
	</Grid>
</Window>
